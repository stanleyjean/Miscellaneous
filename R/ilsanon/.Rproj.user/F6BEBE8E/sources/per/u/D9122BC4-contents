rr <- r_score %>% count(reading_score)
dd <- d_score %>% count(debate_score)
vv <- v_score %>% count(video_score)
mm <- m_score %>% count(material_score)
rr
rr <- rr %>%  add_row(reading_score = "3", n =0, .before = 1)
rr <- rr %>%  add_row(reading_score = "2", n =0, .before = 1)
rr <- rr %>%  add_row(reading_score = "1", n =0, .before = 1)
rr
dd

dd <- dd %>% add_row(debate_score = "2", n = 0, .before = 1)
dd <- dd %>% add_row(debate_score = "1", n = 0, .before = 1)
dd

vv

vv <- vv %>% add_row(video_score = "1", n = 0, .before = 1)
vv

mm <- mm %>% add_row(material_score = "1", n = 0, .before = 1)
mm
rr <- rr %>% mutate(reading_score = as.numeric(reading_score))
dd <- dd %>% mutate(debate_score = as.numeric(debate_score))
