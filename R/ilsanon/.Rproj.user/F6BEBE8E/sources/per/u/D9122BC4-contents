wilcox.test()

wilcox.test(num_scores_value$reading_score, num_scores_value$debate_score, paired=TRUE, exact = FALSE)
wilcox.test(num_scores_value$reading_score, num_scores_value$video_score, paired=TRUE, exact = FALSE)
wilcox.test(num_scores_value$reading_score, num_scores_value$material_score, paired=TRUE, exact = FALSE)

wilcox.test(num_scores_value$debate_score, num_scores_value$video_score, paired=TRUE, exact = FALSE)
wilcox.test(num_scores_value$debate_score, num_scores_value$material_score, paired=TRUE, exact = FALSE)

wilcox.test(num_scores_value$video_score, num_scores_value$material_score, paired=TRUE, exact = FALSE)

kruskal.test(list(rr$reading_score, dd$debate_score, vv$video_score, mm$material_score))
